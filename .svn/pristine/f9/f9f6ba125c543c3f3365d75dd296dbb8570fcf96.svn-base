@charset "utf-8";
    
@font-face {
    font-family: 'NanumSquareL';
    font-style: normal;
    font-weight: 100;
    src: url("../font/NanumSquareL.otf");
    src: url("../font/NanumSquareOTF_acL.otf");
    }

    @font-face {
    font-family: 'NanumSquare';
    font-style: normal;
    font-weight: normal;
    src: url("../font/NanumSquareR.otf");
    src: url("../font/NanumSquareOTF_acR.otf");
    
    }

    @font-face {
    font-family: 'NanumSquareB';
    font-style: normal;
    font-weight: normal;
    src: url("../font/NanumSquareB.otf");
    src: url("../font/NanumSquareOTF_acB.otf");
    }

    @font-face {
    font-family: 'NanumSquareEB';
    font-style: normal;
    font-weight: normal;
    src: url("../font/NanumSquareEB.otf");
    src: url("../font/NanumSquareOTF_acEB.otf");
    }


/*  LAYOUT  */

.normal		{ font-family: 'NanumSquare';}
.bold		{ font-family: 'NanumSquareB';}
.exbold	    { font-family: 'NanumSquareEB'; }
.light		{ font-family: 'NanumSquareL';}

.d_flex{display: flex;}
.flex_w{flex-wrap: wrap;}

.d_jcsb{justify-content: space-between;}

.d_inline{display: inline-block;}

.txt_c{text-align: center;}

.font12{font-size: 12px;}
.font14{font-size: 14px;}
.font16{font-size: 16px;}
.font17{font-size: 17px;}

.gap10{gap:10px}
.gap35{gap:35px}

/*
아래그림자
 box-shadow: 0px 4px 8px -4px rgba(0,0,0,0.2); */


.word_s{word-spacing: 4px;}
.letter_s{letter-spacing: 20px;}


/*단어로 잘라서 줄바꿈*/
.word_b{word-break: keep-all;}



body{
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    color: #333333;
    font: 15px/1 'NanumSquare', sans-serif ;
}



html, body, div, ul, li, table, th, td, label, form, 
input, textarea, em, address, select, a, span{
	font-size: 15px;
    font-family: 'NanumSquare', sans-serif;    
	color: #333333;
	box-sizing: border-box;
}

h1,h2,h3,h4,h5,h6{
    font-family: 'NanumSquare', sans-serif;
}

h2{
    font-size: 25px;
}

h3{
    font-size: 22px;
}

h4{
    font-size: 17px;
}

h5{
    font-size: 14px;
}

input[type="text"], input[type="password"], input[type="search"], input[type="file"], input[type="number"] {
	box-sizing: border-box;
	padding-left: 10px;
    font-family: 'NanumSquare', sans-serif;   
    border: 1px solid #CCCCCC;
	-webkit-appearance: none; /* Safari and Chrome*/ 
	-moz-appearance: none; /* Firefox*/ 
	-ms-appearance: none;
	-o-appearance:none;
	appearance: none;
}

input[type="file"]{
    position: absolute;
    width: 0;
    height: 0;
    padding: 0;
    overflow: hidden;
    border: 0;
}

textarea{
    margin-left: 16px;
    padding: 10px;
    box-sizing: border-box;
    border: 1px solid #CCCCCC;
    resize: none;
}


input[type="text"], input[type="password"], input[type="number"]{
    margin-left: 16px;
}

input[type="radio"]{
    vertical-align: middle;
    margin-right: 5px;
}


input[type=range]{
    outline: none;
}

input[type=range]:focus {
    outline: none;
}

input[type="search"]{
    vertical-align: middle;
    height: 34px;
    border: 1px solid #cccccc;
    background: url(../img/serch_ico.png) no-repeat right 10px center #fff;
	background-size: 19px 20px;
}

/*인풋search 기본x버튼 없애기*/
input::-ms-reveal, input::-ms-clear{
    display: none;
}
input::-webkit-search-decoration,
input::-webkit-search-cancel-button,
input::-webkit-search-results-button,
input::-webkit-search-results-decoration{
	display:none;
}


input[type="date"]{
    height: 34px;
    padding: 2px 0 0 12px;
    line-height: 34px;
    position: relative;
    background: url(../img/data.png) no-repeat right 10px center / 17px auto;
    background-color: #fff;
    appearance: none; -webkit-appearance: none;
    border: 1px solid #CCCCCC;
    box-sizing: border-box;
}


input[type='checkbox']{
    width: 14px;
    height: 14px;
    margin-left: 4px;
    vertical-align: middle;
}

select{
    font-family: 'NanumSquare', sans-serif;
    font-weight: 400;
    border: 1px solid #CCCCCC;
    padding-left: 10px;
	box-sizing: border-box;
	height: 34px;
    vertical-align: middle;
	appearance: none; -webkit-appearance: none;
	background: url(../img/sel_arr.png) no-repeat right 10px center;
	background-size: 9px 6px;
}

input[type="number"]::-webkit-inner-spin-button,
input[type="number"]::-webkit-outer-spin-button {
	-webkit-appearance: inner-spin-button;    
    opacity: 1;
}

table, th, td{
    vertical-align: middle;
}

th {font-family: 'NanumSquareB';}

::placeholder{
    color: #999999;
}

select::-ms-expand{display:none;} /*모든브라우저에서 고유디자인이 나오게*/
select,textarea,input:focus { outline: none;}

-webkit-font-smoothing: antialiased;
-moz-osx-font-smoothing: grayscale;
